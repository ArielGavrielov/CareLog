{
  "expo": {
    "name": "CareLog",
    "slug": "CareLog",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ariel.CareLog",
      "userInterfaceStyle": "automatic",
      "infoPlist": {
        "NSFaceIDUsageDescription": "CareLog needs to use Face ID / Touch ID to authenticate you"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.ariel.CareLog"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
